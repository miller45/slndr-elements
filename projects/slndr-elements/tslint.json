{
  "extends": "../../tslint.json",
  "rules": {
    "no-inferrable-types": false,
    "no-string-literal": false,
    "jsdoc-format": false,
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ]
  }
}
